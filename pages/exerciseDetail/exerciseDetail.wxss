  margin-top: 18rpx;
  border-radius: 16rpx;
}
.hotel-detail__title{
  height: 88rpx;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: left;
  color: rgba(0,0,0,0.90);
  box-sizing: border-box;
  padding-left: 24rpx;
}
.hotel-detail__con{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.con_con{
  display: flex;
  flex-wrap: wrap;
  border-top: 4rpx solid #f5f5f5;
  width: 686rpx;
  box-sizing: border-box;
  padding-top: 24rpx;
}
.hotel-detail_item{
  width: 343rpx;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: left;
  color: #111111;
  margin-bottom: 58rpx;
}
.hotel-detail_item:nth-child(5){
  margin-right: 0;
}
.hotel-detail_item>image{
  width: 140rpx;
  height: 100rpx;
  opacity: 1;
  background: rgba(0,0,0,0.00);
  border-radius: 8rpx;
}
.liaoj{
  font-size: 12px;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  text-align: left;
  color: rgba(0,0,0,0.90);
}
.in-detail{
  margin-top: 30rpx;
}
.hotel-detail1__con{
  margin-top: 24rpx;
  font-size: 22rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
  line-height: 44rpx;
  box-sizing: border-box;
  padding: 0 28rpx;
}
.tt{
  font-size: 20rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  text-align: left;
  color: #999999;
}
.detail_item_right{
  margin-left: 26rpx;
