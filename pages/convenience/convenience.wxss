/* pages/convenience/convenience.wxss */
page{
    background: #f5f5f5;
}
.page{
    box-sizing: border-box;
    padding: 64rpx 20rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 28rpx;
    font-family: PingFang SC, PingFang SC-Bold;
    font-weight: 700;
    text-align: left;
    color: #111;
}
.h-title{
    width: 334rpx;
    height: 46rpx;
    opacity: 1;
    background: #a0c3ca;
    border-top-left-radius: 40rpx;
    border-top-right-radius: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 24rpx;
    font-family: PingFang SC, PingFang SC-Bold;
    font-weight: 700;
    text-align: left;
    color: #ffffff;
    margin-top: 20rpx;
}
.h-table{
    background: #ffffff;
    border-radius: 12rpx;
    padding-top: 40rpx;
}
.tab1{
    display: flex;
    font-size: 20rpx;
    font-family: PingFang SC, PingFang SC-Regular;
    font-weight: 700;
    text-align: left;
    color: #000000;
    text-align: center;
    margin-bottom: 20rpx;
    box-sizing: border-box;
    justify-content: space-between;
    width: 706rpx;
}
.tab2{
    display: flex;
    font-size: 20rpx;
    font-family: PingFang SC, PingFang SC-Regular;
    font-weight: 400;
    text-align: left;
    color: #000000;
    text-align: center;
    box-sizing: border-box;
    justify-content: space-between;
    width: 706rpx;
    padding:  32rpx 5rpx;
    align-items: center;
    border-bottom: 1rpx solid #f5f5f5;
}
.tab2>view,.tab1>view{
    box-sizing: border-box;
}

.tab2:last-child{
    border-bottom: none;
}
.c1{
    width: 93rpx;
    padding-left: 10rpx;
}
.c2{
    width: 105rpx;
}
.c3{
    width: 138rpx;
    font-family: PingFang SC, PingFang SC-Bold;
    font-weight: 700;
    text-align: left;
    color: #4c88d6;
    text-decoration: underline;
    padding-left: 10rpx;
}
.c4{
    width: 210rpx;
    text-align: left;
    margin-left: 20rpx;
    padding-left: 10rpx;
}
.c5{
    width: 120rpx;
}
.cc{
    border-left: 2rpx solid #d6d6d6;
}
.cy{
    margin-top: 60rpx;
}

.logo{
    width: 158rpx;
    height: 96rpx;
    opacity: 1;
    background: rgba(0,0,0,0.00);
    margin-bottom: 28rpx;
}