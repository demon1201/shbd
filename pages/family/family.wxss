/* pages/family/family.wxss */
page{
    background: #f5f5f5;
}
.page{
    width: 100%;
    box-sizing: border-box;
    padding: 30rpx 20rpx;
} 
.cont{
    width: 100%;
    background-color: #fff;
    border-radius: 20rpx;
    box-sizing: border-box;
    padding: 0 20rpx;
    
}
.cont-item{
    display: flex;
    border-bottom: 1rpx #f5f5f5 solid;
    height: 120rpx; 
    align-items:  center;   
}
.item-left{
    height: 80rpx;
    width: 80rpx;
    background-color: #f5f5f5;
    font-size: 22rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    border-radius: 10rpx;
}
.item-right{
    margin-left: 20rpx;
    font-size: 20rpx;
    color: #333;
}
.t2{
    color: rgb(133, 132, 132);
}